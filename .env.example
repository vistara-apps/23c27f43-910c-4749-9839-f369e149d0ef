# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# Environment (production or development)
NEXT_PUBLIC_ENVIRONMENT=development

# Creator wallet address to receive tips
NEXT_PUBLIC_CREATOR_ADDRESS=0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb

# x402 API URL for gasless transactions (optional)
# If not provided, users will pay gas themselves
NEXT_PUBLIC_X402_API_URL=https://x402-api-url.example.com
